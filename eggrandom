-- Hapus GUI lama jika ada
for _, obj in pairs(game.CoreGui:GetChildren()) do
	if obj.Name == "EggRandomizerGUI" or obj.Name == "JToggleLogo" or obj.Name == "EggRandomizerLoading" then
		obj:Destroy()
	end
end

-- Loading Screen
