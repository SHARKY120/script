-- Tombol Close
local closeBtn = Instance.new("TextButton", main)
closeBtn.Size = UDim2.new(0, 30, 0, 30)
closeBtn.Position = UDim2.new(1, -35, 0, 5)
closeBtn.Text = "X"
closeBtn.Font = Enum.Font.GothamBold
closeBtn.TextSize = 16
closeBtn.TextColor3 = Color3.fromRGB(255, 100, 100)
closeBtn.BackgroundTransparency = 1

-- Logo J muncul setelah ditutup
closeBtn.MouseButton1Click:Connect(function()
	gui.Enabled = false
	if not game.CoreGui:FindFirstChild("JToggleLogo") then
		local logo = Instance.new("TextButton", game.CoreGui)
		logo.Name = "JToggleLogo"
		logo.Size = UDim2.new(0, 40, 0, 40)
		logo.Position = UDim2.new(0, 10, 0.2, 0)
		logo.Text = "J"
		logo.Font = Enum.Font.GothamBlack
		logo.TextSize = 18
		logo.TextColor3 = Color3.fromRGB(0, 200, 255)
		logo.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
		Instance.new("UICorner", logo).CornerRadius = UDim.new(1, 0)
		logo.MouseButton1Click:Connect(function()
			gui.Enabled = true
			logo:Destroy()
		end)
	end
end)

-- ESP Toggle
local toggle = Instance.new("TextButton", main)
toggle.Size = UDim2.new(0, 120, 0, 30)
toggle.Position = UDim2.new(0, 10, 0, 60)
toggle.Text = "ðŸ”˜ ESP: OFF"
