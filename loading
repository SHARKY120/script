-- Ganti TextBox jadi TextLabel input click untuk Delta compatibility
local function createFakeInput(parent, placeholder)
	local bg = Instance.new("Frame", parent)
	bg.Size = UDim2.new(0.9, 0, 0, 30)
	bg.Position = UDim2.new(0.05, 0, 0, 40)
	bg.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
	bg.Name = "FakeInput"
	Instance.new("UICorner", bg).CornerRadius = UDim.new(0, 6)
	local stroke = Instance.new("UIStroke", bg)
	stroke.Color = Color3.fromRGB(50, 50, 50)

	local txt = Instance.new("TextLabel", bg)
	txt.Size = UDim2.new(1, -10, 1, 0)
	txt.Position = UDim2.new(0, 5, 0, 0)
	txt.BackgroundTransparency = 1
	txt.Text = placeholder
	txt.TextColor3 = Color3.new(1, 1, 1)
	txt.Font = Enum.Font.Gotham
	txt.TextScaled = true
	txt.TextXAlignment = Enum.TextXAlignment.Left
	txt.TextWrapped = true

	bg.MouseEnter:Connect(function() bg.BackgroundColor3 = Color3.fromRGB(30,30,30) end)
	bg.MouseLeave:Connect(function() bg.BackgroundColor3 = Color3.fromRGB(25,25,25) end)

	bg.MouseButton1Click:Connect(function()
		local input = Instance.new("TextBox", bg)
		input.BackgroundTransparency = 1
		input.Size = txt.Size
		input.Position = txt.Position
		input.Text = ""
		input.TextColor3 = Color3.new(1, 1, 1)
		input.Font = Enum.Font.Gotham
		input.TextScaled = true
		input.TextXAlignment = Enum.TextXAlignment.Left
		input.ClearTextOnFocus = false
		input.Text = ""
		input.FocusLost:Connect(function()
			if input.Text == "" then
				txt.Text = placeholder
			else
				txt.Text = input.Text
			end
			input:Destroy()
		end)
		txt.Text = ""
		input:CaptureFocus()
	end)

	return txt
end

-- Ganti input Pet & Seed
local petInput = createFakeInput(petPage, "Pet Name")
local seedInput = createFakeInput(seedPage, "Seed Name")

-- Ulangi tombol spawn agar pakai input baru
petPage:FindFirstChild("SpawnButton").MouseButton1Click:Connect(function()
	local name = petInput.Text
	if name == "" or name == "Pet Name" then
		createAlert("Please fill pet name", Color3.fromRGB(255, 50, 50))
	else
		createAlert("Successful to spawn " .. name, Color3.fromRGB(50, 255, 100))
		spawnSound:Play()
	end
end)

seedPage:FindFirstChild("SpawnButton").MouseButton1Click:Connect(function()
	local name = seedInput.Text
	if name == "" or name == "Seed Name" then
		createAlert("Please fill seed name", Color3.fromRGB(255, 50, 50))
	else
		createAlert("Successful to spawn " .. name .. " seed", Color3.fromRGB(50, 255, 100))
		spawnSound:Play()
	end
end)

-- Perbaiki posisi alert agar selalu stack di kanan tanpa ikut alert sebelumnya
local function newAlertText(message, color)
	local totalAlerts = #mainGui:GetChildren()
	local alert = Instance.new("TextLabel", mainGui)
	alert.Size = UDim2.new(0, 250, 0, 30)
	alert.Position = UDim2.new(1, -260, 0.5, -160 + (totalAlerts * -35))
	alert.BackgroundColor3 = color
	alert.BackgroundTransparency = 0.3
	alert.Text = message
	alert.Font = Enum.Font.GothamBold
	alert.TextScaled = true
	alert.TextColor3 = Color3.new(1, 1, 1)
	alert.AnchorPoint = Vector2.new(0, 0)
	alert.ZIndex = 50
	Instance.new("UICorner", alert).CornerRadius = UDim.new(0, 6)
	game:GetService("Debris"):AddItem(alert, 3)
end

-- Ganti createAlert agar pakai fungsi baru
createAlert = newAlertText
