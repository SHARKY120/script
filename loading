	menuFrame.Visible = true
	toggleBtn.Visible = false
end)
-- Fungsi alert bertumpuk
local function createAlert(text, isError)
	local existing = #mainGui:GetChildren()
	local alert = Instance.new("TextLabel", mainGui)
	alert.Size = UDim2.new(0, 240, 0, 32)
	alert.Position = UDim2.new(1, -250, 0.5, -100 - (existing * 38))
	alert.AnchorPoint = Vector2.new(0,0)
	alert.BackgroundColor3 = isError and Color3.fromRGB(255, 60, 60) or Color3.fromRGB(50, 255, 100)
	alert.BackgroundTransparency = 0.25
	alert.Font = Enum.Font.GothamBold
	alert.TextScaled = true
	alert.TextColor3 = Color3.new(1,1,1)
	alert.Text = text
	Instance.new("UICorner", alert).CornerRadius = UDim.new(0, 6)

	game:GetService("Debris"):AddItem(alert, 3)
end

