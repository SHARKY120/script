-- Hapus loading screen sebelumnya jika ada
if game.CoreGui:FindFirstChild("FullBlueLoadingScreen") then
    game.CoreGui:FindFirstChild("FullBlueLoadingScreen"):Destroy()
end

-- Sound efek spawn
local spawnSound = Instance.new("Sound")
spawnSound.SoundId = "rbxassetid://6026984224"
spawnSound.Volume = 1
spawnSound.Name = "SpawnSound"
spawnSound.Parent = game:GetService("SoundService")

-- Sound selesai loading
local finishSound = Instance.new("Sound")
finishSound.SoundId = "rbxassetid://9118823102"
finishSound.Volume = 1
finishSound.Parent = game:GetService("SoundService")

-- Buat GUI
local screenGui = Instance.new("ScreenGui", game.CoreGui)
screenGui.Name = "FullBlueLoadingScreen"
screenGui.IgnoreGuiInset = true
screenGui.ResetOnSpawn = false

local TweenService = game:GetService("TweenService")

-- Frame utama
local bg = Instance.new("Frame", screenGui)
bg.Size = UDim2.new(0, 200, 0, 100)
bg.Position = UDim2.new(0.5, 0, 0.5, 0)
bg.AnchorPoint = Vector2.new(0.5, 0.5)
bg.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
bg.BackgroundTransparency = 1
bg.BorderSizePixel = 0
Instance.new("UICorner", bg).CornerRadius = UDim.new(0, 12)

local stroke = Instance.new("UIStroke", bg)
stroke.Thickness = 3
stroke.Color = Color3.fromRGB(0, 170, 255)
stroke.Transparency = 0.2
-- Glow belakang
local glow = Instance.new("ImageLabel", bg)
glow.AnchorPoint = Vector2.new(0.5, 0.5)
glow.Position = UDim2.new(0.5, 0, 0.5, 0)
glow.Size = UDim2.new(1.5, 0, 2, 0)
glow.BackgroundTransparency = 1
glow.Image = "rbxassetid://200527618"
glow.ImageColor3 = Color3.fromRGB(0, 153, 255)
glow.ImageTransparency = 0.8
glow.ZIndex = 0

coroutine.wrap(function()
	while glow.Parent do
		TweenService:Create(glow, TweenInfo.new(2, Enum.EasingStyle.Sine), {ImageTransparency = 0.65}):Play()
		wait(2)
		TweenService:Create(glow, TweenInfo.new(2, Enum.EasingStyle.Sine), {ImageTransparency = 0.8}):Play()
		wait(2)
	end
end)()

-- Judul dan versi
local title = Instance.new("TextLabel", bg)
title.Size = UDim2.new(1, 0, 0, 40)
title.Position = UDim2.new(0, 0, 0, 20)
title.BackgroundTransparency = 1
title.Text = "JEKAK121 SPAWNER"
title.Font = Enum.Font.GothamBold
title.TextScaled = false
title.TextSize = 38
title.TextColor3 = Color3.new(1, 1, 1)
title.ZIndex = 2

local version = Instance.new("TextLabel", bg)
version.Size = UDim2.new(1, 0, 0, 25)
version.Position = UDim2.new(0, 0, 0, 60)
version.BackgroundTransparency = 1
version.Text = "v1.1.0"
version.Font = Enum.Font.Gotham
version.TextScaled = false
version.TextSize = 18
version.TextColor3 = Color3.fromRGB(200, 200, 200)
version.ZIndex = 2

-- Fade in dan expand
wait(0.3)
TweenService:Create(bg, TweenInfo.new(0.4), { BackgroundTransparency = 0 }):Play()
wait(0.2)
TweenService:Create(bg, TweenInfo.new(0.5, Enum.EasingStyle.Back), { Size = UDim2.new(0, 500, 0, 220) }):Play()
TweenService:Create(title, TweenInfo.new(0.5), { TextSize = 48 }):Play()
TweenService:Create(version, TweenInfo.new(0.5), { TextSize = 20 }):Play()
wait(0.6)

-- Progress bar
local progressBarBg = Instance.new("Frame", bg)
progressBarBg.Size = UDim2.new(0.9, 0, 0.2, 0)
progressBarBg.Position = UDim2.new(0.05, 0, 0, 110)
progressBarBg.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
progressBarBg.ClipsDescendants = true
progressBarBg.ZIndex = 2
Instance.new("UICorner", progressBarBg).CornerRadius = UDim.new(1, 0)

local bgText = Instance.new("TextLabel", bg)
bgText.Size = UDim2.new(1, 0, 0.1, 0)
bgText.Position = UDim2.new(0, 0, 0, 185)
bgText.BackgroundTransparency = 1
bgText.Text = "Tiktok: @JEKAK121"
bgText.Font = Enum.Font.GothamSemibold
bgText.TextScaled = true
bgText.TextColor3 = Color3.fromRGB(180, 200, 220)
bgText.TextTransparency = 0.3
bgText.ZIndex = 1

local progressBar = Instance.new("Frame", progressBarBg)
progressBar.Size = UDim2.new(0, 0, 1, 0)
progressBar.BackgroundColor3 = Color3.fromRGB(0, 180, 255)
progressBar.ZIndex = 3
Instance.new("UICorner", progressBar).CornerRadius = UDim.new(1, 0)

local percentText = Instance.new("TextLabel", progressBarBg)
percentText.Size = UDim2.new(1, 0, 1, 0)
percentText.BackgroundTransparency = 1
percentText.Text = "0%"
percentText.Font = Enum.Font.GothamBold
percentText.TextScaled = true
percentText.TextColor3 = Color3.new(1, 1, 1)
percentText.ZIndex = 4
-- Jalankan progress
coroutine.wrap(function()
	for i = 1, 100 do
		progressBar.Size = UDim2.new(i / 100, 0, 1, 0)
		percentText.Text = tostring(i) .. "%"
		wait(0.03)
	end
	wait(0.2)
	finishSound:Play()

	-- Hapus progress bar
	progressBarBg:Destroy()

	-- Mengecil (tidak transparan)
	TweenService:Create(bg, TweenInfo.new(0.4, Enum.EasingStyle.Sine), { Size = UDim2.new(0, 200, 0, 100) }):Play()
	TweenService:Create(title, TweenInfo.new(0.4), { TextSize = 20 }):Play()
	TweenService:Create(version, TweenInfo.new(0.4), { TextSize = 14 }):Play()

	wait(0.5)
	TweenService:Create(bg, TweenInfo.new(0.3, Enum.EasingStyle.Sine), { BackgroundTransparency = 1 }):Play()
	wait(0.3)
	screenGui:Destroy()
end)()
local CoreGui = game:GetService("CoreGui")

-- GUI utama setelah loading
local mainGui = Instance.new("ScreenGui", CoreGui)
mainGui.Name = "JekakSpawnerUI"
mainGui.ResetOnSpawn = false
mainGui.IgnoreGuiInset = true

-- ALERT SYSTEM
local function createAlert(text, color)
	local count = 0
	for _, v in ipairs(mainGui:GetChildren()) do
		if v:IsA("TextLabel") and v.Name == "Alert" then
			count += 1
		end
	end

	local alert = Instance.new("TextLabel", mainGui)
	alert.Name = "Alert"
	alert.Size = UDim2.new(0, 260, 0, 30)
	alert.Position = UDim2.new(1, -270, 0.5, -140 - (count * 35))
	alert.BackgroundColor3 = color
	alert.BackgroundTransparency = 0.3
	alert.Text = text
	alert.Font = Enum.Font.GothamBold
	alert.TextScaled = true
	alert.TextColor3 = Color3.new(1, 1, 1)
	alert.AnchorPoint = Vector2.new(0, 0)
	alert.ZIndex = 10
	Instance.new("UICorner", alert).CornerRadius = UDim.new(0, 6)

	game:GetService("Debris"):AddItem(alert, 3)
end

-- FRAME UTAMA MENU
local menuFrame = Instance.new("Frame", mainGui)
menuFrame.Size = UDim2.new(0, 280, 0, 190)
menuFrame.Position = UDim2.new(0.5, -140, 0.45, 0)
menuFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 15)
menuFrame.Visible = true
Instance.new("UICorner", menuFrame).CornerRadius = UDim.new(0, 12)
local stroke = Instance.new("UIStroke", menuFrame)
stroke.Color = Color3.fromRGB(0, 170, 255)
stroke.Transparency = 0.2

-- NAV BUTTONS
local tabPet = Instance.new("TextButton", menuFrame)
tabPet.Size = UDim2.new(0.5, 0, 0, 25)
tabPet.Position = UDim2.new(0, 0, 0, 0)
tabPet.Text = "Pet Spawner"
tabPet.Font = Enum.Font.GothamBold
tabPet.TextScaled = true
tabPet.TextColor3 = Color3.new(1, 1, 1)
tabPet.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
Instance.new("UICorner", tabPet).CornerRadius = UDim.new(0, 6)

local tabSeed = tabPet:Clone()
tabSeed.Text = "Seed Spawner"
tabSeed.Position = UDim2.new(0.5, 0, 0, 0)
tabSeed.Parent = menuFrame
-- PET PAGE
local petPage = Instance.new("Frame", menuFrame)
petPage.Size = UDim2.new(1, 0, 1, -30)
petPage.Position = UDim2.new(0, 0, 0, 30)
petPage.BackgroundTransparency = 1
petPage.Visible = true

local petInput = Instance.new("TextBox", petPage)
petInput.Size = UDim2.new(0.9, 0, 0, 30)
petInput.Position = UDim2.new(0.05, 0, 0, 10)
petInput.PlaceholderText = "Pet Name"
petInput.Font = Enum.Font.Gotham
petInput.TextScaled = true
petInput.TextColor3 = Color3.new(1, 1, 1)
petInput.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
Instance.new("UICorner", petInput).CornerRadius = UDim.new(0, 6)

local petBtn = Instance.new("TextButton", petPage)
petBtn.Size = UDim2.new(0.6, 0, 0, 30)
petBtn.Position = UDim2.new(0.2, 0, 0, 55)
petBtn.Text = "Spawn"
petBtn.Font = Enum.Font.GothamBold
petBtn.TextScaled = true
petBtn.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
petBtn.TextColor3 = Color3.new(1, 1, 1)
Instance.new("UICorner", petBtn).CornerRadius = UDim.new(0, 6)

-- Tombol bantuan ?
local helpBtn = Instance.new("TextButton", petPage)
helpBtn.Size = UDim2.new(0, 25, 0, 25)
helpBtn.Position = UDim2.new(0, 10, 1, -35)
helpBtn.Text = "?"
helpBtn.Font = Enum.Font.GothamBold
helpBtn.TextScaled = true
helpBtn.BackgroundColor3 = Color3.fromRGB(0, 50, 100)
helpBtn.TextColor3 = Color3.fromRGB(0, 170, 255)
Instance.new("UICorner", helpBtn).CornerRadius = UDim.new(1, 0)

local helpPopup = Instance.new("TextLabel", mainGui)
helpPopup.Visible = false
helpPopup.Size = UDim2.new(0, 340, 0, 220)
helpPopup.Position = UDim2.new(0.5, -170, 0.5, -110)
helpPopup.BackgroundColor3 = Color3.fromRGB(15, 15, 25)
helpPopup.TextColor3 = Color3.new(1, 1, 1)
helpPopup.TextScaled = true
helpPopup.TextWrapped = true
helpPopup.TextYAlignment = Enum.TextYAlignment.Top
helpPopup.Text = [[‚ö†Ô∏èTHINGS YOU NEED TO KNOW BEFORE EXECUTING THE SCRIPT‚ö†Ô∏è

You must be in public server
Don't use new accounts, use your main
Better if you already own the pet:
- Queen Bee üêù
