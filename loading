Instance.new("UICorner", helpPopup).CornerRadius = UDim.new(0, 10)

helpBtn.MouseButton1Click:Connect(function()
	helpPopup.Visible = not helpPopup.Visible
end)

-- SEED PAGE
local seedPage = petPage:Clone()
seedPage.Parent = menuFrame
seedPage.Visible = false
local seedInput = seedPage:FindFirstChildOfClass("TextBox")
local seedBtn = seedPage:FindFirstChildOfClass("TextButton")
seedInput.PlaceholderText = "Seed Name"

-- Tab switching
tabPet.MouseButton1Click:Connect(function()
	petPage.Visible = true
	seedPage.Visible = false
end)
tabSeed.MouseButton1Click:Connect(function()
	seedPage.Visible = true
	petPage.Visible = false
end)
petBtn.MouseButton1Click:Connect(function()
	local txt = petInput.Text
	if txt == "" then
		createAlert("Please fill pet name", Color3.fromRGB(255, 50, 50))
	else
		createAlert("Successful to spawn " .. txt, Color3.fromRGB(50, 255, 100))
		spawnSound:Play()
	end
end)

seedBtn.MouseButton1Click:Connect(function()
	local txt = seedInput.Text
	if txt == "" then
		createAlert("Please fill seed name", Color3.fromRGB(255, 50, 50))
	else
		createAlert("Successful to spawn " .. txt .. " seed", Color3.fromRGB(50, 255, 100))
		spawnSound:Play()
	end
end)

-- Tombol X untuk hide menu
local closeBtn = Instance.new("TextButton", menuFrame)
closeBtn.Size = UDim2.new(0, 25, 0, 25)
closeBtn.Position = UDim2.new(1, -30, 0, 5)
closeBtn.Text = "X"
closeBtn.Font = Enum.Font.GothamBold
closeBtn.TextScaled = true
closeBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
closeBtn.TextColor3 = Color3.new(1, 1, 1)
Instance.new("UICorner", closeBtn).CornerRadius = UDim.new(1, 0)

-- Tombol logo 'J'
local toggleBtn = Instance.new("TextButton", mainGui)
toggleBtn.Size = UDim2.new(0, 45, 0, 45)
toggleBtn.Position = UDim2.new(0, 10, 1, -70)
toggleBtn.Text = "J"
toggleBtn.Font = Enum.Font.GothamBlack
toggleBtn.TextScaled = true
toggleBtn.BackgroundColor3 = Color3.fromRGB(0, 50, 100)
toggleBtn.TextColor3 = Color3.fromRGB(0, 170, 255)
toggleBtn.Visible = false
Instance.new("UICorner", toggleBtn).CornerRadius = UDim.new(1, 0)
local toggleStroke = Instance.new("UIStroke", toggleBtn)
toggleStroke.Color = Color3.fromRGB(0, 170, 255)
toggleStroke.Thickness = 2
toggleStroke.Transparency = 0.2

-- Toggle aksi
closeBtn.MouseButton1Click:Connect(function()
	menuFrame.Visible = false
	toggleBtn.Visible = true
end)

toggleBtn.MouseButton1Click:Connect(function()
	menuFrame.Visible = true
	toggleBtn.Visible = false
end)
