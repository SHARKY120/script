-- Hapus loading screen sebelumnya jika ada
pcall(function()
    if game.CoreGui:FindFirstChild("FullBlueLoadingScreen") then
        game.CoreGui:FindFirstChild("FullBlueLoadingScreen"):Destroy()
    end
end)

-- Sound efek spawn
local spawnSound = Instance.new("Sound", game:GetService("SoundService"))
spawnSound.SoundId = "rbxassetid://6026984224"
spawnSound.Volume = 1

-- Sound selesai loading
local finishSound = Instance.new("Sound", game:GetService("SoundService"))
finishSound.SoundId = "rbxassetid://9118823102"
finishSound.Volume = 1

-- GUI loading ringan
local screenGui = Instance.new("ScreenGui", game.CoreGui)
screenGui.Name = "FullBlueLoadingScreen"
screenGui.IgnoreGuiInset = true
screenGui.ResetOnSpawn = false

local frame = Instance.new("Frame", screenGui)
frame.AnchorPoint = Vector2.new(0.5, 0.5)
frame.Position = UDim2.new(0.5, 0, 0.5, 0)
frame.Size = UDim2.new(0, 200, 0, 100)
frame.BackgroundColor3 = Color3.fromRGB(15, 15, 25)

local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1, 0, 0.5, 0)
title.Position = UDim2.new(0, 0, 0, 0)
title.Text = "JEKAK121 SPAWNER"
title.Font = Enum.Font.GothamBold
title.TextColor3 = Color3.new(1,1,1)
title.TextScaled = true
title.BackgroundTransparency = 1

local label = Instance.new("TextLabel", frame)
label.Size = UDim2.new(1, 0, 0.5, 0)
label.Position = UDim2.new(0, 0, 0.5, 0)
label.Text = "Tiktok: @jekak121"
label.Font = Enum.Font.Gotham
label.TextColor3 = Color3.new(0.6,0.6,0.6)
label.TextScaled = true
label.BackgroundTransparency = 1

wait(2)
finishSound:Play()
screenGui:Destroy()
-- Floating GUI Spawner
local gui = Instance.new("ScreenGui", game.CoreGui)
gui.Name = "SpawnerUI"

local function createMenu(titleText, isPet)
    local frame = Instance.new("Frame", gui)
    frame.Size = UDim2.new(0, 250, 0, 140)
    frame.Position = UDim2.new(0.5, -125, isPet and 0.3 or 0.5, 0)
    frame.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
    frame.Name = isPet and "PetSpawner" or "SeedSpawner"

    local title = Instance.new("TextLabel", frame)
    title.Size = UDim2.new(1, -40, 0, 30)
    title.Position = UDim2.new(0, 10, 0, 5)
    title.Text = titleText
    title.Font = Enum.Font.GothamBold
    title.TextColor3 = Color3.new(1,1,1)
    title.TextScaled = true
    title.BackgroundTransparency = 1
    local input = Instance.new("TextBox", frame)
    input.Size = UDim2.new(0.9, 0, 0, 30)
    input.Position = UDim2.new(0.05, 0, 0, 45)
    input.PlaceholderText = isPet and "Pet Name" or "Seed Name"
    input.Font = Enum.Font.Gotham
    input.TextColor3 = Color3.new(1,1,1)
    input.BackgroundColor3 = Color3.fromRGB(40,40,40)
    input.TextScaled = true

    local button = Instance.new("TextButton", frame)
    button.Size = UDim2.new(0.6, 0, 0, 30)
    button.Position = UDim2.new(0.2, 0, 0, 85)
    button.Text = "Spawn"
    button.Font = Enum.Font.GothamBold
    button.TextColor3 = Color3.new(1,1,1)
    button.BackgroundColor3 = Color3.fromRGB(0,170,255)
    button.TextScaled = true

    button.MouseButton1Click:Connect(function()
        local name = input.Text
        if name ~= "" then
            local alert = Instance.new("TextLabel", gui)
            alert.Size = UDim2.new(0, 200, 0, 30)
            alert.Position = UDim2.new(1, -210, 0.5, -80 + #gui:GetChildren()*35)
            alert.Text = "Successful to spawn " .. name .. (isPet and "" or " seed")
            alert.TextColor3 = Color3.new(1,1,1)
            alert.Font = Enum.Font.GothamBold
            alert.BackgroundColor3 = Color3.fromRGB(50, 255, 100)
            alert.BackgroundTransparency = 0.3
            alert.TextScaled = true
            spawnSound:Play()
            game:GetService("Debris"):AddItem(alert, 3)
        end
    end)
    local close = Instance.new("TextButton", frame)
    close.Size = UDim2.new(0, 25, 0, 25)
    close.Position = UDim2.new(1, -30, 0, 5)
    close.Text = "X"
    close.Font = Enum.Font.GothamBold
    close.TextScaled = true
    close.BackgroundColor3 = Color3.fromRGB(60,60,60)
    close.TextColor3 = Color3.new(1,1,1)
    close.MouseButton1Click:Connect(function()
        frame.Visible = false
        toggle.Visible = true
    end)

    if isPet then
        local help = Instance.new("TextButton", frame)
        help.Size = UDim2.new(0, 25, 0, 25)
        help.Position = UDim2.new(0, 5, 0, 5)
        help.Text = "?"
        help.Font = Enum.Font.GothamBold
        help.TextScaled = true
        help.BackgroundColor3 = Color3.fromRGB(0, 50, 100)
        help.TextColor3 = Color3.fromRGB(0,170,255)

        local popup = Instance.new("TextLabel", gui)
        popup.Size = UDim2.new(0, 300, 0, 200)
        popup.Position = UDim2.new(0.5, -150, 0.5, -100)
        popup.Text = [[‚ö†Ô∏èTHINGS YOU NEED TO KNOW‚ö†Ô∏è
- Only Work On Public server only 
- Use main account don't use new accoun because it will ruin 
  the code
- Work 100% but better if you have pet already owned:
  - Queen Bee üêù
  - Dragonfly üêâ
  - RedFox ü¶ä
  - Raccoon ü¶ù
- Wait 6-10 mins before leaving, don't leave immediately]]
        popup.Font = Enum.Font.Gotham
        popup.TextColor3 = Color3.new(1,1,1)
        popup.BackgroundColor3 = Color3.fromRGB(15,15,25)
        popup.Visible = false
        popup.TextWrapped = true
        popup.TextScaled = true

        help.MouseButton1Click:Connect(function()
            popup.Visible = not popup.Visible
        end)
    end

    return frame
end
-- Tombol toggle logo J
local toggle = Instance.new("TextButton", gui)
toggle.Size = UDim2.new(0, 40, 0, 40)
toggle.Position = UDim2.new(0, 10, 0, 10)
toggle.Text = "J"
toggle.Font = Enum.Font.GothamBold
toggle.TextScaled = true
toggle.BackgroundColor3 = Color3.fromRGB(0, 50, 100)
toggle.TextColor3 = Color3.fromRGB(0,170,255)

local petMenu = createMenu("Pet Spawner", true)
local seedMenu = createMenu("Seed Spawner", false)

toggle.MouseButton1Click:Connect(function()
    petMenu.Visible = true
    seedMenu.Visible = true
    toggle.Visible = false
end)
