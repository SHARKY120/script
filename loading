spawnPetBtn.MouseButton1Click:Connect(function()
	local name = petInput.Text
	if name == nil or name == "" then
		createAlert("Please fill pet name!", Color3.fromRGB(255, 80, 80))
	else
		createAlert("Successful to spawn " .. name, Color3.fromRGB(50, 255, 100))
		if spawnSound then spawnSound:Play() end
	end
end)

