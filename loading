-- Fungsi update highlight tombol aktif
local function highlightButton(activeBtn, inactiveBtn)
	activeBtn.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
	inactiveBtn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
end

