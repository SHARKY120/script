-- Fungsi update tampilan page & highlight tombol
local function switchPage(toPage)
	currentPage = toPage
	petPage.Visible = (currentPage == "Pet")
	seedPage.Visible = (currentPage == "Seed")

	-- Highlight aktif
	pagePetBtn.BackgroundColor3 = currentPage == "Pet" and Color3.fromRGB(0, 170, 255) or Color3.fromRGB(40, 40, 40)
	pagePetBtn.TextColor3 = currentPage == "Pet" and Color3.new(1, 1, 1) or Color3.fromRGB(180, 180, 180)

	pageSeedBtn.BackgroundColor3 = currentPage == "Seed" and Color3.fromRGB(0, 170, 255) or Color3.fromRGB(40, 40, 40)
	pageSeedBtn.TextColor3 = currentPage == "Seed" and Color3.new(1, 1, 1) or Color3.fromRGB(180, 180, 180)
end

-- Saat tombol page diklik
pagePetBtn.MouseButton1Click:Connect(function()
	switchPage("Pet")
end)

pageSeedBtn.MouseButton1Click:Connect(function()
	switchPage("Seed")
end)

-- Tombol X (close menu)
local closeBtn = Instance.new("TextButton", mainFrame)
closeBtn.Size = UDim2.new(0, 25, 0, 25)
closeBtn.Position = UDim2.new(1, -30, 0, 5)
closeBtn.Text = "X"
closeBtn.Font = Enum.Font.GothamBold
closeBtn.TextScaled = true
closeBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
closeBtn.TextColor3 = Color3.new(1, 1, 1)
Instance.new("UICorner", closeBtn).CornerRadius = UDim.new(1, 0)

closeBtn.MouseButton1Click:Connect(function()
	mainFrame.Visible = false
	logoJ.Visible = true
end)

