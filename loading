	toggleBtn.Visible = false
end)

-- Fungsi highlight page aktif
local function switchPage(page)
	currentPage = page
	if page == "Pet" then
		pagePetBtn.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
		pageSeedBtn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	else
		pagePetBtn.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
		pageSeedBtn.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
	end
end

pagePetBtn.MouseButton1Click:Connect(function()
	switchPage("Pet")
end)

pageSeedBtn.MouseButton1Click:Connect(function()
	switchPage("Seed")
end)

-- Set awal page aktif Pet
switchPage("Pet")
-- Frame konten Pet dan Seed (berpindah saat ganti page)
local contentFrame = Instance.new("Frame", mainFrame)
contentFrame.Size = UDim2.new(1, -20, 1, -50)
contentFrame.Position = UDim2.new(0, 10, 0, 40)
contentFrame.BackgroundTransparency = 1

-- Buat dua konten frame: petPage dan seedPage
local petPage = Instance.new("Frame", contentFrame)
petPage.Size = UDim2.new(1, 0, 1, 0)
petPage.BackgroundTransparency = 1
petPage.Visible = true

local seedPage = Instance.new("Frame", contentFrame)
seedPage.Size = UDim2.new(1, 0, 1, 0)
seedPage.BackgroundTransparency = 1
seedPage.Visible = false

-- Fungsi update tampilan page
local function updatePageDisplay()
	petPage.Visible = currentPage == "Pet"
	seedPage.Visible = currentPage == "Seed"
end

pagePetBtn.MouseButton1Click:Connect(updatePageDisplay)
pageSeedBtn.MouseButton1Click:Connect(updatePageDisplay)

-- ===== PET PAGE =====
local petLabel = Instance.new("TextLabel", petPage)
petLabel.Size = UDim2.new(1, 0, 0, 30)
petLabel.Position = UDim2.new(0, 0, 0, 5)
petLabel.Text = "Pet Name:"
petLabel.Font = Enum.Font.GothamBold
petLabel.TextScaled = true
petLabel.TextColor3 = Color3.new(1, 1, 1)
petLabel.BackgroundTransparency = 1

local selectedPet = ""
local petList = {"Queen Bee üêù", "Dragonfly üêâ", "RedFox ü¶ä", "Raccoon ü¶ù", "Mimic Octopus üêô"}

-- Buat list pilihan pet
local dropdown = Instance.new("Frame", petPage)
dropdown.Size = UDim2.new(1, 0, 0, #petList * 26)
dropdown.Position = UDim2.new(0, 0, 0, 40)
dropdown.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
dropdown.ClipsDescendants = true
Instance.new("UICorner", dropdown).CornerRadius = UDim.new(0, 6)

for i, petName in ipairs(petList) do
	local option = Instance.new("TextButton", dropdown)
	option.Size = UDim2.new(1, 0, 0, 25)
	option.Position = UDim2.new(0, 0, 0, (i - 1) * 26)
	option.Text = petName
	option.Font = Enum.Font.Gotham
	option.TextScaled = true
	option.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
	option.TextColor3 = Color3.new(1, 1, 1)
	Instance.new("UICorner", option).CornerRadius = UDim.new(0, 4)

	option.MouseButton1Click:Connect(function()
		selectedPet = petName
		createAlert("Successful to spawn " .. selectedPet, false)
		spawnSound:Play()
	end)
end

-- Tombol Spawn Pet
local petSpawnBtn = Instance.new("TextButton", petPage)
petSpawnBtn.Size = UDim2.new(0.6, 0, 0, 30)
petSpawnBtn.Position = UDim2.new(0.2, 0, 1, -35)
petSpawnBtn.Text = "Spawn"
petSpawnBtn.Font = Enum.Font.GothamBold
petSpawnBtn.TextScaled = true
petSpawnBtn.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
petSpawnBtn.TextColor3 = Color3.new(1, 1, 1)
Instance.new("UICorner", petSpawnBtn).CornerRadius = UDim.new(0, 6)

petSpawnBtn.MouseButton1Click:Connect(function()
	if selectedPet == "" then
		createAlert("Please fill pet name", true)
	else
		createAlert("Successful to spawn " .. selectedPet, false)
		spawnSound:Play()
	end
end)
