spawnPetBtn.MouseButton1Click:Connect(function()
	local txt = petInput.Text
	if txt == "" then
		createAlert("Please fill pet name", true)
	else
		createAlert("Successful to spawn " .. txt)
		spawnSound:Play()
	end
end)

